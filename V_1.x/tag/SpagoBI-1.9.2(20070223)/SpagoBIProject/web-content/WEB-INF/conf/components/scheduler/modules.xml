<?xml version="1.0" encoding="ISO-8859-1"?>
<MODULES>
    
    <MODULE class="it.eng.spagobi.scheduler.modules.SchedulerModule" name="SchedulerModule">
     	<CONFIG/>
    </MODULE>
    
    <MODULE class="it.eng.spagobi.scheduler.modules.SchedulerGUIModule" name="SchedulerGUIModule">
     	<CONFIG rows="10" title="scheduler.SchedulationListTitle" >
	      <QUERIES/>
	      <COLUMNS>
	      	<COLUMN label="scheduler.TriggerName" name="TriggerName" />
	      	<COLUMN label="scheduler.TriggerGroup" name="TriggerGroup" hidden="true" />
	        <COLUMN label="scheduler.triggerDescription" name="triggerDescription" />
			<!-- <COLUMN label="triggerCalendarName" name="triggerCalendarName" /> -->
			<COLUMN label="scheduler.triggerStartTime" name="triggerStartTime" />
			<COLUMN label="scheduler.triggerEndTime" name="triggerEndTime" />
	      </COLUMNS>
	      <CAPTIONS>
	      	<DETAIL_CAPTION confirm="FALSE" image="/img/detail.gif" label="scheduler.detail">
	          <PARAMETER name="MESSAGEDET" scope="" type="ABSOLUTE" value="MESSAGE_GET_OBJECT_SCHEDULATION_DETAILS"/>
	          <PARAMETER name="PAGE" scope="" type="ABSOLUTE" value="SchedulerGUIPage"/>
	          <PARAMETER name="triggerName" scope="LOCAL" type="RELATIVE" value="TriggerName"/>
	          <PARAMETER name="OBJECT_ID" scope="SERVICE_RESPONSE" type="RELATIVE" value="SchedulerGUIModule.OBJECT_ID"/>
	          <PARAMETER name="OBJ_JOB_EXISTS" scope="SERVICE_RESPONSE" type="RELATIVE" value="SchedulerGUIModule.OBJ_JOB_EXISTS"/> 
	        </DETAIL_CAPTION>
	        <DELETE_CAPTION confirm="TRUE" image="/img/erase.gif" label="scheduler.delete">
	          <PARAMETER name="MESSAGEDET" scope="" type="ABSOLUTE" value="MESSAGE_DELETE_OBJECT_SCHEDULE"/>
	          <PARAMETER name="PAGE" scope="" type="ABSOLUTE" value="SchedulerGUIPage"/>
	          <PARAMETER name="triggerName" scope="LOCAL" type="RELATIVE" value="TriggerName"/>
	          <PARAMETER name="triggerGroup" scope="LOCAL" type="RELATIVE" value="TriggerGroup"/>
	          <PARAMETER name="OBJECT_ID" scope="SERVICE_RESPONSE" type="RELATIVE" value="SchedulerGUIModule.OBJECT_ID"/>
	          <PARAMETER name="LIGHT_NAVIGATOR_DISABLED" type="ABSOLUTE" scope="" value="TRUE" />
	        </DELETE_CAPTION>
	      </CAPTIONS>
	      <BUTTONS>
	        <INSERT_BUTTON confirm="TRUE" image="/img/new.png" label="scheduler.newschedule">
	          <PARAMETER name="MESSAGEDET" scope="" type="ABSOLUTE" value="MESSAGE_NEW_OBJECT_SCHEDULATION"/>
	          <PARAMETER name="PAGE" scope="" type="ABSOLUTE" value="SchedulerGUIPage"/>
	          <PARAMETER name="OBJECT_ID" scope="SERVICE_RESPONSE" type="RELATIVE" value="SchedulerGUIModule.OBJECT_ID"/>
	          <PARAMETER name="OBJ_JOB_EXISTS" scope="SERVICE_RESPONSE" type="RELATIVE" value="SchedulerGUIModule.OBJ_JOB_EXISTS"/>
	        </INSERT_BUTTON>
	        <BACK_BUTTON confirm="FALSE" image="/img/back.png" label="scheduler.back">
	          <PARAMETER name="LIGHT_NAVIGATOR_BACK_TO" scope="" type="ABSOLUTE" value="1"/>
	        </BACK_BUTTON>
	      </BUTTONS>
	    </CONFIG>
    </MODULE>
    
</MODULES>
