<?xml version="1.0" encoding="ISO-8859-1"?>
<page-set>

  <page renderer="PageColumnRenderer">
    <owner>exo</owner>
    <name>/home</name>
    <title>Home</title>
    <viewPermission group="/guest" membership="*"/>
    <container width="67%" renderer="ContainerColumnRenderer">
      <container width="67%" renderer="FirstLevelTabRenderer" decorator="page">
        <portlet>
          <title>SpagoBI News</title>
          <windowId>exo:/web/RssPortlet/RssPortletSpagobi</windowId>
          <portlet-style>spagobistyleRSS</portlet-style>
          <showInfoBar>false</showInfoBar>
          <portlet-preferences>  
            <preference>
              <name>#{description}</name>
              <value>This is spagobi news</value>
            </preference>
            <preference>
              <name>url</name>
              <value>/spagobinews.xml</value>
            </preference>
            <preference>
              <name>channel</name>
              <value>News</value>
            </preference>
            <preference>
              <name>item-to-show</name>
              <value>5</value>
            </preference>
          </portlet-preferences>
        </portlet>
        <portlet decorator="community" renderer="default">>
          <title>#{Introduction}</title>
          <windowId>exo:/content/DisplayStaticContent/home-intro</windowId>
          <portlet-style>spagobistyleDSC</portlet-style>
          <showInfoBar>false</showInfoBar>
          <portlet-preferences>
            <preference>
              <name>d</name>
              <value>id=war:/spagobi-static-content/home-intro</value>
            </preference>
          </portlet-preferences>
        </portlet>
        <portlet>
          <title>#{Implementation_Notes}</title>
          <windowId>exo:/content/DisplayStaticContent/implNotes</windowId>
          <portlet-style>spagobistyleDSC</portlet-style>
          <showInfoBar>false</showInfoBar>
          <portlet-preferences>
            <preference>
              <name>a</name>
              <value>id=war:/spagobi-static-content/impl_notes</value>
            </preference>
          </portlet-preferences>
        </portlet>
        <portlet>
          <title>#{Administrator}</title>
          <windowId>exo:/content/DisplayStaticContent/adminmanual</windowId>
          <portlet-style>spagobistyleDSC</portlet-style>
          <showInfoBar>false</showInfoBar>
          <portlet-preferences>
            <preference>
              <name>c</name>
              <value>id=war:/spagobi-static-content/manual_admin</value>
            </preference>
          </portlet-preferences>
        </portlet>
        <portlet>
          <title>#{Developer}</title>
          <windowId>exo:/content/DisplayStaticContent/devmanual</windowId>
          <portlet-style>spagobistyleDSC</portlet-style>
          <showInfoBar>false</showInfoBar>
          <portlet-preferences>
            <preference>
              <name>f</name>
              <value>id=war:/spagobi-static-content/manual_dev</value>
            </preference>
          </portlet-preferences>
        </portlet>
        <portlet>
          <title>#{Tester}</title>
          <windowId>exo:/content/DisplayStaticContent/testmanual</windowId>
          <portlet-style>spagobistyleDSC</portlet-style>
          <showInfoBar>false</showInfoBar>
          <portlet-preferences>
            <preference>
              <name>b</name>
              <value>id=war:/spagobi-static-content/manual_test</value>
            </preference>
          </portlet-preferences>
        </portlet>
        <portlet>
          <title>#{User}</title>
          <windowId>exo:/content/DisplayStaticContent/usermanual</windowId>
          <portlet-style>spagobistyleDSC</portlet-style>
          <showInfoBar>false</showInfoBar>
          <portlet-preferences>
            <preference>
              <name>e</name>
              <value>id=war:/spagobi-static-content/manual_user</value>
            </preference>
          </portlet-preferences>
        </portlet>
      </container>
    </container>
  </page>

  <page>
    <owner>exo</owner>
    <name>/home/sitemap</name>
    <title>Site Map</title>
    <viewPermission group="/guest" membership="*"/>
    <portlet>
      <windowId>exo:/web/PortalInfoPortlet/sitemap</windowId>
      <showInfoBar>false</showInfoBar>
      <portlet-preferences>
        <preference>
          <name>template</name>
          <value>community/sitemap.vm</value>
          <read-only>true</read-only>
        </preference>
      </portlet-preferences>
    </portlet>
  </page>


  <page>
    <owner>exo</owner>
    <name>/home/sbisite</name>
    <title>SpagoBI Web Site</title>
    <viewPermission group="/guest" membership="*"/>
    <portlet width="*">
      <title>SpagoBI Web Site</title>
      <windowId>exo:/content/DisplayStaticContent/sbisite</windowId>
      <portlet-style>spagobistyleDSC</portlet-style>
      <portlet-preferences>
        <preference>
          <name>default</name>
          <value>id=war:/spagobi-static-content/spagobi_website</value>
        </preference>
      </portlet-preferences>
    </portlet>
  </page>

