      <title>SpagoBi Manual</title>
      <windowId>bitest:/content/DisplayStaticContent/manual</windowId>
      <portlet-style>spagobistyleDSC</portlet-style>
      <portlet-preferences>
        <preference>
          <name>default</name>
          <value>id=war:/spagobi-static-content/manual_test</value>
        </preference>
      </portlet-preferences>
    </portlet>
  </page> 


</page-set>
