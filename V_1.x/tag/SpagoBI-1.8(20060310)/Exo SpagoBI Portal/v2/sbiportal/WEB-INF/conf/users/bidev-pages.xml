<?xml version="1.0" encoding="ISO-8859-1"?>
<page-set>
  <page renderer="PageColumnRenderer">
    <owner>bidev</owner>
    <name>/home</name>
    <title>Home</title>
    <viewPermission group="/guest" membership="*"/>
    <container renderer="FirstLevelTabRenderer">    
      <portlet>
        <title>Development Environment</title>
        <windowId>bidev:/spagobi/SBIDevelopmentContext/sbidevcont</windowId>
        <portlet-style>spagobistyle168</portlet-style>
        <showInfoBar>false</showInfoBar>
      </portlet>      
    </container>    
  </page>

  <page>
    <owner>bidev</owner>
    <name>/home/manual</name>
    <title>SpagoBi Manual</title>
    <viewPermission group="/user" membership="owner"/>
    <portlet width="*">
      <title>SpagoBi Manual</title>
      <windowId>bidev:/content/DisplayStaticContent/manual</windowId>
      <portlet-style>spagobistyleDSC</portlet-style>
      <portlet-preferences>
        <preference>
          <name>default</name>
          <value>id=war:/spagobi-static-content/manual_dev</value>
        </preference>
      </portlet-preferences>
    </portlet>
  </page> 


</page-set>
