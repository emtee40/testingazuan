<?xml version="1.0" encoding="ISO-8859-1"?>

<PUBLISHERS>

     <PUBLISHER name="SPAGOBI_LOOP_PUBLISHER">
    	<RENDERING channel="HTTP" mode="" type="LOOP">
      		<RESOURCES>
      			
      			<PARAMETER name="ACTION_NAME" type="ABSOLUTE" scope="" value="SELECT_DATAMART_AND_INIT_NEW_WIZARD_ACTION" />
      			<PARAMETER name="NEW_SESSION" type="ABSOLUTE" scope="" value="TRUE" />
      			
      			<PARAMETER name="SPAGOBI_REQUEST" type="RELATIVE" scope="SERVICE_RESPONSE" value="SPAGOBI_REQUEST" />
      			<PARAMETER name="DATAMART_PROPERTIES" type="RELATIVE" scope="SERVICE_RESPONSE" value="DATAMART_PROPERTIES" />
      			
        	</RESOURCES>
   	 	</RENDERING>
  	</PUBLISHER>
     
     
	<PUBLISHER name="DATA_MART_MAIN_PUBLISHER">
		<RENDERING channel="HTTP" type="JSP" mode="FORWARD">
			<RESOURCES>
				<ITEM prog="0" resource="/jsp/initializer/qbeAppSelectDatasourcePage.jsp" />
			</RESOURCES>
		</RENDERING>
		
		<RENDERING channel="PORTLET" mode="FORWARD" type="JSP">
      		<RESOURCES>
        		<ITEM mode="VIEW" resource="/jsp/initializer/qbeAppSelectDatasourcePage.jsp" />
        		<ITEM mode="EDIT" resource="/jsp/initializer/qbeAppSelectDatasourcePage.jsp" />
      		</RESOURCES>
    	</RENDERING>
	</PUBLISHER>
	
	<PUBLISHER name="LOGIN_PUBLISHER">
		<RENDERING channel="HTTP" type="JSP" mode="FORWARD">
			<RESOURCES>
				<ITEM prog="0" resource="/jsp/initializer/qbeAppLoginPage.jsp" />
			</RESOURCES>
		</RENDERING>
	</PUBLISHER>
	
	
	
	<PUBLISHER name="INDEX_HTML_PUBLISHER">
		<RENDERING channel="HTTP" type="JSP" mode="FORWARD">
			<RESOURCES>
				<ITEM prog="0" resource="/jsp/initializer/qbeAppMainProfiledPage.jsp" />
			</RESOURCES>
		</RENDERING>
	</PUBLISHER>
	
	<PUBLISHER name="QBE_LOOP_START_PUBLISHER">
  		
    	<RENDERING channel="HTTP" mode="" type="LOOP">
      	
      		<RESOURCES>
        		<PARAMETER name="ACTION_NAME" type="RELATIVE" scope="SERVICE_RESPONSE" value="ACTION" />
        		<PARAMETER name="PATH" type="RELATIVE" scope="SERVICE_RESPONSE" value="PATH" />
                <PARAMETER name="NEW_SESSION" type="ABSOLUTE" scope="" value="TRUE" />
			</RESOURCES>
   	 	
   	 	</RENDERING>
   	 	
  	</PUBLISHER>
	
	<!--  START PAGE WITH AUTHORIZATION CHECK ENABLED -->
  	<PUBLISHER name="START_PUBLISHER">
		<RENDERING channel="HTTP" type="JSP" mode="FORWARD">
			<RESOURCES>
				<ITEM prog="0" resource="/jsp/initializer/qbeAppMainProfiledPage.jsp" />
			</RESOURCES>
		</RENDERING>
	</PUBLISHER>
	
	<PUBLISHER name="SERVICE_ERROR_PUBLISHER">
		<RENDERING channel="HTTP" type="JSP" mode="FORWARD">
			<RESOURCES>
				<ITEM prog="0" resource="/jsp/serviceError.jsp" />
			</RESOURCES>
		</RENDERING>
	</PUBLISHER>
	                        	
</PUBLISHERS>
