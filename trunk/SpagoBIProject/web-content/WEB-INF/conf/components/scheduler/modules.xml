<?xml version="1.0" encoding="ISO-8859-1"?>
<MODULES>
    
    <MODULE class="it.eng.spagobi.scheduler.modules.SchedulerModule" name="SchedulerModule">
     	<CONFIG/>
    </MODULE>
    
    <MODULE class="it.eng.spagobi.scheduler.modules.SchedulerGUIModule" name="SchedulerGUIModule">
     	<CONFIG rows="10" title="ChangeMe" >
	      <QUERIES/>
	      <COLUMNS>
	      	<COLUMN label="TriggerName" name="TriggerName" />
	        <COLUMN label="triggerDescription" name="triggerDescription" />
			<COLUMN label="triggerCalendarName" name="triggerCalendarName" />
			<COLUMN label="triggerStartTime" name="triggerStartTime" />
			<COLUMN label="triggerEndTime" name="triggerEndTime" />
	      </COLUMNS>
	      <CAPTIONS>
	      	<DETAIL_CAPTION confirm="FALSE" image="/img/detail.gif" label="ChangeMe">
	          <PARAMETER name="MESSAGEDET" scope="" type="ABSOLUTE" value="MESSAGE_GET_OBJECT_SCHEDULATION_DETAILS"/>
	          <PARAMETER name="PAGE" scope="" type="ABSOLUTE" value="SchedulerGUIPage"/>
	          <PARAMETER name="triggerName" scope="LOCAL" type="RELATIVE" value="TriggerName"/>
	          <PARAMETER name="OBJECT_ID" scope="SERVICE_RESPONSE" type="RELATIVE" value="SchedulerGUIModule.OBJECT_ID"/>
	          <PARAMETER name="OBJ_JOB_EXISTS" scope="SERVICE_RESPONSE" type="RELATIVE" value="SchedulerGUIModule.OBJ_JOB_EXISTS"/>
	        </DETAIL_CAPTION>
	      </CAPTIONS>
	      <BUTTONS>
	        <INSERT_BUTTON confirm="TRUE" image="/img/new.png" label="ChangeMe">
	          <PARAMETER name="MESSAGEDET" scope="" type="ABSOLUTE" value="MESSAGE_NEW_OBJECT_SCHEDULATION"/>
	          <PARAMETER name="PAGE" scope="" type="ABSOLUTE" value="SchedulerGUIPage"/>
	          <PARAMETER name="OBJECT_ID" scope="SERVICE_RESPONSE" type="RELATIVE" value="SchedulerGUIModule.OBJECT_ID"/>
	          <PARAMETER name="OBJ_JOB_EXISTS" scope="SERVICE_RESPONSE" type="RELATIVE" value="SchedulerGUIModule.OBJ_JOB_EXISTS"/>
	        </INSERT_BUTTON>
	        <BACK_BUTTON confirm="FALSE" image="/img/back.png" label="ChangeMe">
	          <PARAMETER name="LIGHT_NAVIGATOR_BACK_TO" scope="" type="ABSOLUTE" value="1"/>
	        </BACK_BUTTON>
	      </BUTTONS>
	    </CONFIG>
    </MODULE>
    
</MODULES>
