<?xml version="1.0"?>
<Schema name="procom">
<!--
  == Definizione MDX Cubo Progetti
  == Autore:                EngiSEL
  == Versione:              1.0.0
  == Data creazione         22/11/2005
  == Data ultima modifica   22/11/2005
-->

<!-- Shared dimensions -->
  
 
 
       
       
  <Dimension name="PERIODO-PROGRAMMA-ASSE-MISURA">
    <Hierarchy hasAll="true" allMemberName="Tutte" primaryKey="FK_MISURA" primaryKeyTable="T030_DW_ABILITAZIONI_PROCOM">
     <Join leftKey="FK_MISURA" rightKey="ID_MISURA">
        
        <Table name="T030_DW_ABILITAZIONI_PROCOM"/>
        <Table name="T030_DW_D_MISURE"/>
     </Join>  
      <Level name="Struttura"     table="T030_DW_ABILITAZIONI_PROCOM"  column="FK_STRUTTURA"      uniqueMembers="true"/>
      <Level name="Periodo"       table="T030_DW_D_MISURE"   hideMemberIf="IfBlankName" column="DE_PERIODO"     uniqueMembers="false"  />
      <Level name="Programma"     table="T030_DW_D_MISURE"   hideMemberIf="IfBlankName" column="DS_PROGRAMMA"   uniqueMembers="false" />
      <Level name="Asse"          table="T030_DW_D_MISURE"   hideMemberIf="IfBlankName" column="DS_ASSE"        uniqueMembers="false" />
      <Level name="Misura"        table="T030_DW_D_MISURE"   hideMemberIf="IfBlankName" column="DE_LIVELLO1"   uniqueMembers="false" />
      <Level name="Sottomisura"   table="T030_DW_D_MISURE"   hideMemberIf="IfBlankName" column="DE_LIVELLO2"   uniqueMembers="false" />
     </Hierarchy>
  </Dimension>      
       

<!-- Cubi -->
  <Cube name="Pagato">
         <Table name="T030_DW_F_PAGATO"/>
        
        
        
        
        <DimensionUsage name="PERIODO-PROGRAMMA-ASSE-MISURA" source="PERIODO-PROGRAMMA-ASSE-MISURA" foreignKey="FK_MISURA"/>
       
         
        
        
       <Dimension name="ANNO">
		<Hierarchy hasAll="true" allMemberName="Tutti" >
                         <Level  name="ANNO"     column="FK_ANNO"  uniqueMembers="true"/>
                         
                </Hierarchy>
        </Dimension>
        
        <!-- Dimensioni -->
       

        
         <!-- Misure -->
             <Measure name="TOTALE PAGAMENTI"               column="TOTALE"                         aggregator="sum"    formatString="#,##0.00"/>
         
         <Measure name="UE PAGAMENTI"                   column="UE"                                     aggregator="sum"    formatString="#,##0.00"/>

         <Measure name="FDR PAGAMENTI"                  column="STATO"                                  aggregator="sum"    formatString="#,##0.00"/>
         
         <Measure name="REGIONE PAGAMENTI"              column="REGIONE"                                aggregator="sum"    formatString="#,##0.00"/>
         
         <Measure name="PRIVATI PAGAMENTI"              column="PAG_PRIVATI"                            aggregator="sum"    formatString="#,##0.00"/>
         
         <Measure name="ALTRI PUBBLICI PAGAMENTI"       column="PAG_ALTRI_PUB"                          aggregator="sum"    formatString="#,##0.00"/>        
      
         <Measure name="NAZIONE PAGAMENTI"              column="NAZIONE"                                aggregator="sum"    formatString="#,##0.00"/>
         
         <Measure name="PUBBLICI PAGAMENTI"             column="PUBBLICI"                               aggregator="sum"    formatString="#,##0.00"/>        


        
       
        
   </Cube>
   
   <Cube name="Impegnato">
         <Table name="T030_DW_F_IMPEGNATO"/>
        <!-- Tabella dei fatti -->
        
        
        
        <DimensionUsage name="PERIODO-PROGRAMMA-ASSE-MISURA" source="PERIODO-PROGRAMMA-ASSE-MISURA" foreignKey="FK_MISURA"/>
       
         <Dimension name="ANNO">
		<Hierarchy hasAll="true" allMemberName="Tutti" >
                         <Level  name="ANNO"     column="FK_ANNO"  uniqueMembers="true"/>
                         
                </Hierarchy>
        </Dimension>
        
        <!-- Dimensioni -->
       

        
         <!-- Misure -->
         <Measure name="TOTALE IMPEGNI"               column="TOTALE"                                 aggregator="sum"    formatString="#,##0.00"/>
         
         <Measure name="UE IMPEGNI"                   column="UE"                                     aggregator="sum"    formatString="#,##0.00"/>

         <Measure name="FDR IMPEGNI"                  column="STATO"                                  aggregator="sum"    formatString="#,##0.00"/>
         
         <Measure name="REGIONE IMPEGNI"              column="REGIONE"                                aggregator="sum"    formatString="#,##0.00"/>
         
         <Measure name="PRIVATI IMPEGNI"              column="PAG_PRIVATI"                            aggregator="sum"    formatString="#,##0.00"/>
         
         <Measure name="ALTRI PUBBLICI IMPEGNI"       column="PAG_ALTRI_PUB"                          aggregator="sum"    formatString="#,##0.00"/>        
      
         <Measure name="NAZIONE IMPEGNI"              column="NAZIONE"                                aggregator="sum"    formatString="#,##0.00"/>
         
         <Measure name="PUBBLICI IMPEGNI"             column="PUBBLICI"                               aggregator="sum"    formatString="#,##0.00"/>        

        
   </Cube>
   
 
  <VirtualCube name="Pagamenti_Impegni">
       <VirtualCubeDimension cubeName="Pagato" name="PERIODO-PROGRAMMA-ASSE-MISURA"/>
       <VirtualCubeDimension cubeName="Impegnato" name="PERIODO-PROGRAMMA-ASSE-MISURA"/>
       <VirtualCubeDimension cubeName="Pagato" name="ANNO"/>
       <VirtualCubeDimension cubeName="Impegnato" name="ANNO"/>
       <VirtualCubeMeasure cubeName="Pagato" name="[Measures].[TOTALE PAGAMENTI]"/>
       <VirtualCubeMeasure cubeName="Impegnato" name="[Measures].[TOTALE IMPEGNI]"/>
       <VirtualCubeMeasure cubeName="Pagato" name="[Measures].[UE PAGAMENTI]"/>
       <VirtualCubeMeasure cubeName="Impegnato" name="[Measures].[UE IMPEGNI]"/>
       <VirtualCubeMeasure cubeName="Pagato" name="[Measures].[FDR PAGAMENTI]"/>
       <VirtualCubeMeasure cubeName="Impegnato" name="[Measures].[FDR IMPEGNI]"/>
       <VirtualCubeMeasure cubeName="Pagato" name="[Measures].[REGIONE PAGAMENTI]"/>
       <VirtualCubeMeasure cubeName="Impegnato" name="[Measures].[REGIONE IMPEGNI]"/>
        <VirtualCubeMeasure cubeName="Pagato" name="[Measures].[PRIVATI PAGAMENTI]"/>
       <VirtualCubeMeasure cubeName="Impegnato" name="[Measures].[PRIVATI IMPEGNI]"/>
       <VirtualCubeMeasure cubeName="Pagato" name="[Measures].[ALTRI PUBBLICI PAGAMENTI]"/>
       <VirtualCubeMeasure cubeName="Impegnato" name="[Measures].[ALTRI PUBBLICI IMPEGNI]"/>
       <VirtualCubeMeasure cubeName="Pagato" name="[Measures].[NAZIONE PAGAMENTI]"/>
       <VirtualCubeMeasure cubeName="Impegnato" name="[Measures].[NAZIONE IMPEGNI]"/>
       <VirtualCubeMeasure cubeName="Pagato" name="[Measures].[PUBBLICI PAGAMENTI]"/>
       <VirtualCubeMeasure cubeName="Impegnato" name="[Measures].[PUBBLICI IMPEGNI]"/>
     
       
       
  
  </VirtualCube>

  
  
        
        

  
 

  
  
  
       
</Schema>
