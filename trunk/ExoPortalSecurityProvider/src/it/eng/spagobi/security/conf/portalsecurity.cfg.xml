<EXO_PORTAL_SECURITY>
	<PROFILE_ATTRIBUTES>
		
		<ATTRIBUTE name="product" default="" source="absolute" />
		<ATTRIBUTE name="city" default="" source="absolute" />
		<ATTRIBUTE name="month" default="9" source="absolute" />
		<ATTRIBUTE name="limit" default="5" source="absolute" />
		<ATTRIBUTE name="Birthday" default="" source="exo"  exoname="user.bdate" />
		
		<USER-PROFILES>
			<USER name="biadmin">
				<ATTRIBUTE name="product" value="Food" />
				<ATTRIBUTE name="city" value="{,{Berkeley,Ballard}}" />
			</USER>
			<USER name="bidev">
				<ATTRIBUTE name="product" value="Drink" />
				<ATTRIBUTE name="city" value="{,{Albany,Altadena,Colma,Arcadia}}" />
			</USER>
			<USER name="biuser">
				<ATTRIBUTE name="product" value="Food" />
				<ATTRIBUTE name="city" value="{,{Novato,Renton}}" />
			</USER>
		</USER-PROFILES>
		
		
