<shortcuts>
  <programGroup defaultName="SpagoBI Platform" location="applications"/>
  <shortcut
     name="Start SpagoBI Platform"
     programGroup="yes"
     desktop="yes"
     applications="no"
     startMenu="yes"
     startup="no"
     target="$INSTALL_PATH/StartSpagoBI.sh"
     commandLine=""
     initialState="normal"
     iconFile="$INSTALL_PATH/StartSpagoBI.png"
