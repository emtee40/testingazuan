  <page renderer="PageColumnRenderer">
    <owner>exo</owner>
    <name>/home/community/news</name>
    <title>News</title>
    <state>maximized</state>
    <viewPermission>any</viewPermission>
    <editPermission>owner</editPermission>
    <container renderer="TabRenderer">    
      <portlet>
        <title>The Server Side</title>
        <showInfoBar>false</showInfoBar>
        <windowId>exo:/web/RssPortlet/RssPortlet</windowId>
        <portlet-style>rss</portlet-style>
        <portlet-preferences>
          <preference>
            <name>description</name>
            <value>This is the news from internet</value>
          </preference>
          <preference>
            <name>url</name>
            <value>http://www.theserverside.com/rss/theserverside-1.0.rdf</value>
          </preference>
          <preference>
            <name>channel</name>
            <value>News</value>
          </preference>
          <preference>
            <name>item-to-show</name>
            <value>5</value>
          </preference>
        </portlet-preferences>
      </portlet>
      <container id="blogs" renderer="TabRenderer">  
        <container id="Portals" renderer="TabRenderer">  
          <portlet>
            <title>Punit Pandey</title>
            <showInfoBar>false</showInfoBar>
            <windowId>exo:/web/RssPortlet/exo_blog_punit</windowId>
            <portlet-style>default</portlet-style>
            <portlet-preferences>
              <preference>
                <name>description</name>
                <value>This is the news from internet</value>
              </preference>
              <preference>
                <name>url</name>
                <value>http://www.jroller.com/rss/portlets</value>
              </preference>
              <preference>
                <name>channel</name>
                <value>News</value>
              </preference>
              <preference>
                <name>item-to-show</name>
                <value>5</value>
              </preference>
            </portlet-preferences>
          </portlet>        
        </container> 
        <container id="Wiki" renderer="TabRenderer">  
          <portlet>
            <title>Ludovic Dubost</title>
            <showInfoBar>false</showInfoBar>
            <windowId>exo:/web/RssPortlet/exo_blog_ludo</windowId>
            <portlet-style>default</portlet-style>
            <portlet-preferences>
              <preference>
                <name>description</name>
                <value>This is the news from internet</value>
              </preference>
              <preference>
                <name>url</name>
                <value>http://www.ludovic.org/blog/index.rdf</value>
              </preference>
              <preference>
                <name>channel</name>
                <value>News</value>
              </preference>
              <preference>
                <name>item-to-show</name>
                <value>5</value>
              </preference>
            </portlet-preferences>
          </portlet>        
        </container>        
      </container>
    </container>
  </page>

