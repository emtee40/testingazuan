<?xml version="1.0" encoding="ISO-8859-1"?>
<page-set>
  <page>
    <owner>exo</owner>
    <name>/home</name>
    <title>Home</title>
    <viewPermission>any</viewPermission>
    <editPermission>owner</editPermission>
    <container width="100%" renderer="ContainerColumnRenderer">
    <portlet width="70%">
      <portlet-style>exo-content</portlet-style>
      <showInfoBar>false</showInfoBar>
      <title>Home</title>
      <windowId>exo:/content/DisplayStaticContent/news</windowId>
      <portlet-preferences>
	<preference>
          <name>d</name>
          <value>title=content.title.introduction</value>
          <value>uri=war:/portal/spagobi-static-content/home-intro</value>
          <value>encoding=ISO-8859-1</value>
          <read-only>false</read-only>
        </preference>
        <preference>
          <name>a</name>
          <value>title=implNotes</value>
          <value>uri=war:/portal/spagobi-static-content/impl_notes</value>
          <value>encoding=ISO-8859-1</value>
          <read-only>false</read-only>
        </preference>
        <preference>
          <name>c</name>
          <value>title=adminmanual</value>
          <value>uri=war:/portal/spagobi-static-content/manual_admin</value>
          <value>encoding=ISO-8859-1</value>
          <read-only>false</read-only>
        </preference>        
		<preference>
          <name>f</name>
          <value>title=devmanual</value>
          <value>uri=war:/portal/spagobi-static-content/manual_dev</value>
          <value>encoding=ISO-8859-1</value>
          <read-only>false</read-only>
        </preference>
        <preference>
          <name>b</name>
          <value>title=testmanual</value>
          <value>uri=war:/portal/spagobi-static-content/manual_test</value>
          <value>encoding=ISO-8859-1</value>
          <read-only>false</read-only>
        </preference>
        <preference>
          <name>e</name>
          <value>title=usermanual</value>
          <value>uri=war:/portal/spagobi-static-content/manual_user</value>
          <value>encoding=ISO-8859-1</value>
          <read-only>false</read-only>
        </preference>
      </portlet-preferences>
    </portlet>
    <portlet width="30%" renderer="BoxPortletRenderer" decorator="ic3-box">
        <title>SpagoBI News</title>
        <showInfoBar>false</showInfoBar>
        <windowId>exo:/web/RssPortlet/RssPortletSpagobi</windowId>
        <portlet-style>rss</portlet-style>
        <portlet-preferences>
          <preference>
            <name>description</name>
            <value>This is spagobi news</value>
          </preference>
          <preference>
            <name>url</name>
            <value>/spagobinews.xml</value>
          </preference>
          <preference>
            <name>channel</name>
            <value>News</value>
          </preference>
          <preference>
            <name>item-to-show</name>
            <value>5</value>
          </preference>
        </portlet-preferences>
      </portlet>
    </container> 
  </page>


