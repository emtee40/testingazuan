<?xml version="1.0" encoding="ISO-8859-1"?>
<page-set>
  <page>
    <owner>bitest</owner>
    <name>/home</name>
    <title>Home</title>
    <viewPermission>any</viewPermission>
    <editPermission>owner</editPermission>
     <container renderer="TabRenderer">    
 	<portlet>
        	<portlet-style>default</portlet-style>
        	<title>Business Intelligence Documents</title>
                <showInfoBar>false</showInfoBar> 
        	<windowId>spagobi:/spagobi/SBIFunctionality/sbifunct</windowId>
        </portlet>      
    </container>    
  </page>

  <page>
    <owner>bitest</owner>
    <name>/home/manual</name>
    <title>SpagoBi Manual</title>
    <viewPermission>any</viewPermission>
    <editPermission>owner</editPermission>
    <portlet width="*">
      <portlet-style>exo-content</portlet-style>
      <showInfoBar>false</showInfoBar> 
      <title>SpagoBi Manual</title>
      <windowId>bitest:/content/DisplayStaticContent/manual</windowId>
      <portlet-preferences>
        <preference>
          <name>default</name>
          <value>title=Default Content</value>
          <value>uri=war:/portal/spagobi-static-content/manual_test</value>
          <value>encoding=ISO-8859-1</value>
        </preference>
      </portlet-preferences>
    </portlet>
  </page> 


  <page>
    <owner>bitest</owner>
    <name>/home/community</name>
    <title>Community</title>
    <viewPermission>any</viewPermission>
    <editPermission>owner</editPermission>
    <portlet width="*">
      <portlet-style>default</portlet-style>
      <title>Community</title>
      <windowId>bitest:/content/DisplayStaticContent/community</windowId>
      <portlet-preferences>
        <preference>
          <name>default</name>
          <value>title=Default Content</value>
          <value>uri=war:/portal/static-content/community/charter</value>
          <value>encoding=UTF-8</value>
        </preference>
      </portlet-preferences>
    </portlet>
  </page>

  <page renderer="PageColumnRenderer">
    <owner>bitest</owner>
    <name>/home/community/news</name>
    <title>News</title>
    <state>maximized</state>
    <viewPermission>any</viewPermission>
    <editPermission>owner</editPermission>
    <container renderer="TabRenderer">    
      <portlet>
        <title>The Server Side</title>
        <showInfoBar>false</showInfoBar>
        <windowId>bitest:/web/RssPortlet/RssPortlet</windowId>
        <portlet-style>rss</portlet-style>
        <portlet-preferences>
          <preference>
            <name>description</name>
            <value>This is the news from internet</value>
          </preference>
          <preference>
            <name>url</name>
            <value>http://www.theserverside.com/rss/theserverside-1.0.rdf</value>
          </preference>
          <preference>
            <name>channel</name>
            <value>News</value>
          </preference>
          <preference>
            <name>item-to-show</name>
            <value>5</value>
          </preference>
        </portlet-preferences>
      </portlet>
      <container id="blogs" renderer="TabRenderer">  
        <container id="Portals" renderer="TabRenderer">  
          <portlet>
            <title>Punit Pandey</title>
            <showInfoBar>false</showInfoBar>
            <windowId>bitest:/web/RssPortlet/exo_blog_punit</windowId>
            <portlet-style>default</portlet-style>
            <portlet-preferences>
              <preference>
                <name>description</name>
                <value>This is the news from internet</value>
              </preference>
              <preference>
                <name>url</name>
                <value>http://www.jroller.com/rss/portlets</value>
              </preference>
              <preference>
                <name>channel</name>
                <value>News</value>
              </preference>
              <preference>
                <name>item-to-show</name>
                <value>5</value>
              </preference>
            </portlet-preferences>
          </portlet>        
        </container> 
        <container id="Wiki" renderer="TabRenderer">  
          <portlet>
            <title>Ludovic Dubost</title>
            <showInfoBar>false</showInfoBar>
            <windowId>bitest:/web/RssPortlet/exo_blog_ludo</windowId>
            <portlet-style>default</portlet-style>
            <portlet-preferences>
              <preference>
                <name>description</name>
                <value>This is the news from internet</value>
              </preference>
              <preference>
                <name>url</name>
                <value>http://www.ludovic.org/blog/index.rdf</value>
              </preference>
              <preference>
                <name>channel</name>
                <value>News</value>
              </preference>
              <preference>
                <name>item-to-show</name>
                <value>5</value>
              </preference>
            </portlet-preferences>
          </portlet>        
        </container>        
      </container>
    </container>
  </page>

  <page>
    <owner>bitest</owner>
    <name>/home/community/forum</name>
    <title>Forum</title>
    <state>maximized</state>
    <viewPermission>any</viewPermission>
    <editPermission>owner</editPermission>
    <portlet>
      <portlet-style>forum</portlet-style>
      <title>Forum</title>
      <windowId>bitest:/communications/Forum/forum</windowId>
    </portlet>
  </page>
  
  <page>
    <owner>bitest</owner>
    <name>/home/community/messages</name>
    <title>Messages</title>
    <state>maximized</state>
    <viewPermission>any</viewPermission>
    <editPermission>owner</editPermission>
    <portlet>
      <title>Message Center</title>
      <portlet-style>msc</portlet-style>
      <windowId>bitest:/communications/MessageCenter/message-center</windowId>
    </portlet>
  </page>  

  <page>
    <owner>bitest</owner>
    <name>/home/community/wiki</name>
    <title>Wiki</title>
    <viewPermission>any</viewPermission>
    <editPermission>owner</editPermission>
    <container renderer="ContainerColumnRenderer" width="*">
      <portlet width="*">
        <title>Wiki</title>
        <windowId>bitest:/xwiki/xwiki/wiki</windowId>
      </portlet>
    </container>
  </page>

  <page>
    <owner>bitest</owner>
    <name>/home/my-portal</name>
    <title>My Portal</title>
    <viewPermission>any</viewPermission>
    <editPermission>owner</editPermission>
    <portlet>
      <portlet-style>register</portlet-style>
      <title>My Portal</title>
      <windowId>bitest:/user/RegisterPortlet/window-register</windowId>
    </portlet>
  </page>

  <page>
    <owner>bitest</owner>
    <name>/home/sitemap</name>
    <title>Site Map</title>
    <viewPermission>any</viewPermission>
    <editPermission>owner</editPermission>
    <portlet>
      <portlet-style>sitemap</portlet-style>
      <title>Site Map</title>
      <windowId>bitest:/nav/Sitemap/sitemap</windowId>
    </portlet>
  </page>
  
  <page>
    <owner>bitest</owner>
    <name>/home/search</name>
    <title>Search</title>
    <viewPermission>any</viewPermission>
    <editPermission>owner</editPermission>
    <portlet>
      <portlet-style>search</portlet-style>
      <title>Search</title>
      <windowId>bitest:/content/SearchPortlet/search</windowId>
    </portlet>
  </page>  

  <page>
    <owner>bitest</owner>
    <name>/mobile</name>
    <title>Mobile</title>
    <viewPermission>any</viewPermission>
    <editPermission>owner</editPermission>
    <portlet>
      <title>Home</title>
      <windowId>bitest:/content/DisplayStaticContent/mobile</windowId>
      <portlet-preferences>
        <preference>
          <name>default</name>
          <value>title=Default Content</value>
          <value>uri=war:/portal/static-content/mobile-home</value>
          <value>encoding=UTF-8</value>
        </preference>
      </portlet-preferences>
    </portlet>
  </page>
</page-set>
