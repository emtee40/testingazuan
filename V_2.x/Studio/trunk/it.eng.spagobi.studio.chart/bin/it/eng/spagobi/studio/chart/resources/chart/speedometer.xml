<DIALCHART type='speedometer' name='Title'>
  <DIMENSION width='280' height='280'/>
  <STYLE_TITLE font='Arial' size='14' color='#000000'/>  
  <STYLE_VALUE_LABEL font='Arial' size='18' color='#00EEEE'/> 
  <STYLE_TICK_LABELS font='Arial' size='12' color='#FF0000'/> 
  <CONF>    
    <PARAMETER name='lower' value='-100'/>
    <PARAMETER name='upper' value='100'/>
    <PARAMETER name='increment' value='20'/>
    <PARAMETER name='minortickcount' value='5'/>
    <PARAMETER name='dialtextuse' value='false'/>
  </CONF>
    <INTERVALS>
      <INTERVAL  min='-100.0' max='-60.0' color='#FF0000'/>
      <INTERVAL  min='-60.0' max='-20.0' color='#FFFF00'/>
      <INTERVAL  min='-20.0' max='20.0' color='#00FF00'/>
      <INTERVAL  min='20.0' max='60.0' color='#FFFF00'/>
      <INTERVAL  min='60.0' max='100.0' color='#FF0000'/>
    </INTERVALS>

</DIALCHART>