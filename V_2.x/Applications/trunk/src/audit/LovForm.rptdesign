                </structure>
            </list-property>
            <property name="queryText">SELECT SBI_LOV.LOV_ID, SBI_LOV.NAME, SBI_LOV.LABEL, SBI_LOV.DESCR, VALUE_CD, LOV_PROVIDER,
        (SELECT COUNT(DISTINCT SBI_PARUSE.PAR_ID) 
         FROM SBI_PARUSE
         WHERE SBI_PARUSE.LOV_ID = ? ) AS TOT_PAR
FROM     SBI_LOV, SBI_DOMAINS, SBI_PARUSE
WHERE    SBI_LOV.INPUT_TYPE_CD = SBI_DOMAINS.VALUE_CD AND
	 SBI_LOV.LOV_ID =?
GROUP BY SBI_LOV.LOV_ID, SBI_LOV.NAME, SBI_LOV.LABEL, SBI_LOV.DESCR, VALUE_CD, LOV_PROVIDER</property>
