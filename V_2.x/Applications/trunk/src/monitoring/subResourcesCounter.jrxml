ON MODEL_RESOUCE.KPI_MODEL_INST = MODEL_INST.KPI_MODEL_INST
LEFT JOIN SBI_RESOURCES ARESOURCE
ON MODEL_RESOUCE.RESOURCE_ID = ARESOURCE.RESOURCE_ID
WHERE MODEL_INST.KPI_MODEL_INST = $P{KPI_MODEL_INST}
GROUP BY MODEL_INST.KPI_MODEL_INST]]></queryString>

