    "common-XXX" :
    {
      "let" :
      {
        "LOCALES" : ["en", "en_US", "de", "de_DE", "fr", "fr_FR"] 
      },
      
      "library" :
      [
        {
          "manifest" : "../../framework/Manifest.json",
          "uri"      : "../../../../../framework"
        },

        {
          "manifest" : "Manifest.demo.json",
          "uri"      : "../../.."
        }
      ],

      "include" :
      [
        "YYY",
        "qx.log.appender.Native",
        "qx.log.appender.Console",
        "qx.dev.ObjectSummary",
        "qx.dev.Pollution"
      ],

      "require" :
      {
      	"qx.ui.core.Widget" : ["qx.theme.Classic","qx.theme.Modern"]
      },

      "themes" :
      {
        "qx.icontheme" : ["Oxygen", "Tango"],
        "qx.theme"     : ["Classic", "Modern"]
      },

      "cache" :
      {
        "compile" : "../../cache"
      },

      "settings" :
      {
        "qx.application" : "YYY",
        "qx.allowUrlVariants": "true",
        "qx.allowUrlSettings" : "true",
        "qx.legacy.theme" : "qx.legacy.theme.ClassicRoyale"
      }
    },

    "source-XXX" :
    {
      "extend" : ["common-XXX"],

      "variants" :
      {
        "qx.theme" : ["qx.theme.Modern", "qx.theme.Classic"]
      },

      "compile-source" :
      {
        "file" : "source/script/YYY-{qx.theme}.js",
        "locales" : "${LOCALES}",
        "root" : "source/demo/yyy"
      }
    },

    "build-XXX" :
    {
      "extend" : ["common-XXX"],

      "variants" :
      {
        "qx.debug" : ["off"],
        "qx.aspects" : ["off"],
        "qx.theme" : ["qx.theme.Modern", "qx.theme.Classic"]
      },

      "compile-dist" :
      {
        "file" : "build/script/YYY-{qx.theme}.js",
        "uri"  : "../../script/YYY-{qx.theme}.js",
        "resourceUri" : "../..",
        "root" : "build/demo/yyy",
        "locales" : "${LOCALES}",
        "optimize" : [ "variables", "basecalls", "privates", "strings" ]
      }
    }
