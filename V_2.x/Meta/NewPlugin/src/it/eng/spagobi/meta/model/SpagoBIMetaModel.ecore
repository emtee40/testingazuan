<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="physical"
    nsURI="http:///it/eng/spagobi/meta/model/physical.ecore" nsPrefix="it.eng.spagobi.meta.model.physical">
  <eClassifiers xsi:type="ecore:EClass" name="PhysicalColumn">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="comment" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="dataType" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EShort"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="typeName" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="size" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="octectLength" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="decimalDigits" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="radix" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="defaultValue" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="nullable" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="position" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="table" lowerBound="1" eType="#//PhysicalTable"
        eOpposite="#//PhysicalTable/columns"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PhysicalForeignKey">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="sourceName" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sourceTable" lowerBound="1"
        eType="#//PhysicalTable" eOpposite="#//PhysicalTable/foreignKeys"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sourceColumns" lowerBound="1"
        upperBound="-1" eType="#//PhysicalColumn"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="destinationName" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="destinationTable" lowerBound="1"
        eType="#//PhysicalTable" eOpposite="#//PhysicalTable/reverseForeignKeys"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="destinationColumns" lowerBound="1"
        upperBound="-1" eType="#//PhysicalColumn"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="model" eType="#//PhysicalModel"
        eOpposite="#//PhysicalModel/foreignKeys"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PhysicalModel">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="databaseName" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="databaseVersion" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="catalog" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="schema" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="tables" upperBound="-1"
        eType="#//PhysicalTable" containment="true" eOpposite="#//PhysicalTable/model"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="primaryKeys" upperBound="-1"
        eType="#//PhysicalPrimaryKey" eOpposite="#//PhysicalPrimaryKey/model"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="foreignKeys" upperBound="-1"
        eType="#//PhysicalForeignKey" eOpposite="#//PhysicalForeignKey/model"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PhysicalPrimaryKey">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="model" lowerBound="1" eType="#//PhysicalModel"
        eOpposite="#//PhysicalModel/primaryKeys"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="table" eType="#//PhysicalTable"
        eOpposite="#//PhysicalTable/primaryKey"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="columns" lowerBound="1"
        upperBound="-1" eType="#//PhysicalColumn"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PhysicalTable">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="comment" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="model" lowerBound="1" eType="#//PhysicalModel"
        eOpposite="#//PhysicalModel/tables"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="columns" upperBound="-1"
        eType="#//PhysicalColumn" eOpposite="#//PhysicalColumn/table"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="primaryKey" eType="#//PhysicalPrimaryKey"
        eOpposite="#//PhysicalPrimaryKey/table"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="foreignKeys" upperBound="-1"
        eType="#//PhysicalForeignKey" eOpposite="#//PhysicalForeignKey/sourceTable"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="reverseForeignKeys" upperBound="-1"
        eType="#//PhysicalForeignKey" eOpposite="#//PhysicalForeignKey/destinationTable"/>
  </eClassifiers>
</ecore:EPackage>
