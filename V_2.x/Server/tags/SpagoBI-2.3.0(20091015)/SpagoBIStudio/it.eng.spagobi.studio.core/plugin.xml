   </extension>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="it.eng.spagobi.studio.core.actionSet">
         <menu
               label="Sample &amp;Menu"
               id="sampleMenu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               label="&amp;Sample Action"
               icon="icons/sample.gif"
               class="it.eng.spagobi.studio.core.actions.SampleAction"
               tooltip="Hello, Eclipse world"
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="it.eng.spagobi.studio.core.actions.SampleAction">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml">
      </toc>
      <toc
            file="testToc.xml"
            primary="true">
      </toc>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <category
            name="Sample File Import"
            id="it.eng.spagobi.studio.core.importWizards.sampleCategory">
      </category>
      <wizard
            name="Import File"
            icon="icons/sample.gif"
            category="it.eng.spagobi.studio.core.importWizards.sampleCategory"
            class="it.eng.spagobi.studio.core.importWizards.ImportWizard"
            id="it.eng.spagobi.studio.core.importWizards.ImportWizard">
         <description>
            Import a file from the local file system into the workspace.
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="Sample Wizards"
            id="it.eng.spagobi.studio.core">
      </category>
      <wizard
            name="Multi-page Editor file"
            icon="icons/sample.gif"
            category="it.eng.spagobi.studio.core"
            class="it.eng.spagobi.studio.core.wizards.SampleNewWizard"
            id="it.eng.spagobi.studio.core.wizards.SampleNewWizard">
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.core.resources.IFile"
            id="it.eng.spagobi.studio.core.contribution1">
         <menu
               label="New Submenu"
               path="additions"
               id="it.eng.spagobi.studio.core.menu1">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               label="New Action"
               class="it.eng.spagobi.studio.core.popup.actions.NewAction"
               menubarPath="it.eng.spagobi.studio.core.menu1/group1"
               enablesFor="1"
               id="it.eng.spagobi.studio.core.newAction">
         </action>
      </objectContribution>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="it.eng.spagobi.studio.core.preferences.PreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            objectClass="org.eclipse.core.resources.IFile"
            name="Sample Page"
            nameFilter="*.*"
            class="it.eng.spagobi.studio.core.properties.SamplePropertyPage"
            id="it.eng.spagobi.studio.core.properties.samplePropertyPage">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.splashHandlers">
      <splashHandler
            class="it.eng.spagobi.studio.core.splashHandlers.InteractiveSplashHandler"
            id="it.eng.spagobi.studio.core.splashHandlers.interactive">
      </splashHandler>
      <splashHandlerProductBinding
            splashId="it.eng.spagobi.studio.core.splashHandlers.interactive"
            productId="org.eclipse.sdk.ide">
      </splashHandlerProductBinding>
   </extension>
   <extension
         point="org.eclipse.ui.intro.configExtension">
      <configExtension
            content="intro/sample.xml"
            configId="org.eclipse.ui.intro.universalConfig">
      </configExtension>
   </extension>

   <extension
         point="org.eclipse.ui.perspectiveExtensions">
