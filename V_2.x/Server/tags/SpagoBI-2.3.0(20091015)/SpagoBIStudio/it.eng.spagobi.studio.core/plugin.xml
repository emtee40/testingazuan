<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="spagoBIStudioBuilder"
         name="SpagoBIStudio Project Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="true">
         <run
               class="it.eng.spagobi.studio.core.builder.SpagoBIStudioBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="spagoBIStudioNature"
         name="SpagoBIStudio Project Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="it.eng.spagobi.studio.core.builder.SpagoBIStudioNature">
         </run>
      </runtime>
      <builder
            id="it.eng.spagobi.studio.core.spagoBIStudioBuilder">
      </builder>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            name="SpagoBI"
            id="it.eng.spagobi.studio.core">
      </category>
      <view
            name="SpagoBIStudio"
            icon="icons/sbiproject.gif"
            category="it.eng.spagobi.studio.core"
            class="it.eng.spagobi.studio.core.views.SampleView"
            id="it.eng.spagobi.studio.core.views.SampleView">
      </view>
   </extension>
