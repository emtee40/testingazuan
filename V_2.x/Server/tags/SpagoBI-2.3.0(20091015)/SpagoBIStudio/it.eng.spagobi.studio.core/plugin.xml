	<extension
         point="org.eclipse.ui.views">
      <view
            name="SpagoBIStudio Resource Navigator"
            category="it.eng.spagobi.studio.core"
            class="org.eclipse.ui.navigator.CommonNavigator"
            id="it.eng.spagobi.studio.core.views.ResourceNavigator"
            icon="icons/sbiproject.gif">
      </view>
   </extension>

	<extension
         point="org.eclipse.ui.navigator.viewer">
       <viewerActionBinding
            viewerId="it.eng.spagobi.studio.core.views.ResourceNavigator">
         <includes>
             <actionExtension pattern="org.eclipse.ui.navigator.resources.*" />
         </includes>
       </viewerActionBinding>
       <viewerContentBinding 
            viewerId="it.eng.spagobi.studio.core.views.ResourceNavigator">
          <includes>
       	     <contentExtension pattern="org.eclipse.ui.navigator.resourceContent" />		       	      
	     	 <contentExtension pattern="org.eclipse.ui.navigator.resources.filters.*"/>
             <contentExtension pattern="org.eclipse.ui.navigator.resources.linkHelper"/>
             <contentExtension pattern="org.eclipse.ui.navigator.resources.workingSets"/>
          </includes>
       </viewerContentBinding>
   </extension>
	
   <extension point="org.eclipse.ui.perspectives">
	   <perspective
	         name="SpagoBI"
	         class="it.eng.spagobi.studio.core.perspectives.SpagoBIPerspective"
	         id="it.eng.spagobi.studio.core.perspectives.SpagoBIPerspective"
	         icon="icons/sbiproject.gif" >
	   </perspective>
	</extension>
    <extension
         point="org.eclipse.ui.preferencePages">
      <page
            name="SpagoBI"
            class="it.eng.spagobi.studio.core.preferences.SpagoBIPreferencePage"
            id="it.eng.spagobi.studio.core.preferences.SpagoBIPreferencePage">
      </page>
    </extension>
    <extension
         point="org.eclipse.ui.preferencePages">
      <page
            name="SpagoBI Editors Configuration"
            class="it.eng.spagobi.studio.core.preferences.EditorsConfigurationPreferencePage"
            id="it.eng.spagobi.studio.core.preferences.EditorsConfigurationPreferencePage">
      </page>
    </extension>   
    <extension point="org.eclipse.ui.newWizards">
      <category id="SpagoBI" name="SpagoBI" />
      <wizard
            category="SpagoBI"
            class="it.eng.spagobi.studio.core.wizards.NewSpagoBIProjectWizard"
            hasPages="true"
            icon="icons/sbiproject.gif"
            id="it.eng.spagobi.studio.core.wizards.newSpagoBIProjectWizard"
            name="SpagoBI Project"
            project="true" 
            finalPerspective="it.eng.spagobi.studio.core.perspectives.SpagoBIPerspective" />
   </extension>
   <extension
          point="org.eclipse.ui.popupMenus">
       <viewerContribution
             id="it.eng.spagobi.studio.core.popupmenu.ResourceNavigatorContribution"
             targetID="it.eng.spagobi.studio.core.views.ResourceNavigator">
         <menu
            id="it.eng.spagobi.studio.core.main.menu" 
            path="additions" 
            label="SpagoBI"> 
            <separator name="SpagoBI"/> 
         </menu> 
         <action
            id="it.eng.spagobi.studio.core.actions.DownloadDocumentAction" 
            label="Download document from server"
            style="push"
            menubarPath="it.eng.spagobi.studio.core.main.menu/SpagoBI" 
            class="it.eng.spagobi.studio.core.actions.DownloadDocumentAction" />
         <action
            id="it.eng.spagobi.studio.core.actions.DeployDocumentAction" 
            label="Deploy document into server"
            style="push"
            menubarPath="it.eng.spagobi.studio.core.main.menu/SpagoBI" 
            class="it.eng.spagobi.studio.core.actions.DeployDocumentAction" /> 
         <action
            id="it.eng.spagobi.studio.core.actions.ShowDocumentInfoAction" 
            label="Show document information"
            style="push"
            menubarPath="it.eng.spagobi.studio.core.main.menu/SpagoBI" 
            class="it.eng.spagobi.studio.core.actions.ShowDocumentInfoAction" /> 
       </viewerContribution>
    </extension>

   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            class="it.eng.spagobi.studio.core.properties.PropertyPage"
            id="it.eng.spagobi.studio.core.propertyPage"
            name="SpagoBI"
            nameFilter="*.sbi*">
      </page>
   </extension>
   
