   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               label="Sample Menu"
               mnemonic="M"
               id="it.eng.spagobi.studio.core.menus.sampleMenu">
            <command
                  commandId="it.eng.spagobi.studio.core.commands.sampleCommand"
                  mnemonic="S"
