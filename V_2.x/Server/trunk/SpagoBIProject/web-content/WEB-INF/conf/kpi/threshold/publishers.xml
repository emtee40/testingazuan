<?xml version="1.0" encoding="ISO-8859-1"?>
<PUBLISHERS>
  <PUBLISHER name="ManageThresholdsPublisher">
    	<RENDERING channel="PORTLET" mode="FORWARD" type="JSP">
      		<RESOURCES>
        		<ITEM mode="VIEW" resource="/WEB-INF/jsp/kpi/threshold/manageThresholds.jsp"/>
      		</RESOURCES>
    	</RENDERING>
