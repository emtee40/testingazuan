<DIALCHART type='meter' name='Customer Capture'>
  <DIMENSION width='200' height='200'/>
  <STYLE_TITLE font='Arial' size='14' color='#000000'/>  
  <STYLE_VALUE_LABEL font='Arial' size='9' color='#000000'/> 
  <STYLE_TICK_LABELS font='Arial' size='9' color='#000000'/> 
  <CONF>
    <PARAMETER name='orientation' value='horizontal'/>
    <PARAMETER name='lower' value='0'/>
    <PARAMETER name='upper' value='100'/>
    <PARAMETER name='increment' value='25'/>
    <PARAMETER name='minortickcount' value='25'/>
  </CONF>
    <INTERVALS>
       <INTERVAL  min='0.0' max='30.0' color='#FF3333' label ='Bad'/>
      <INTERVAL  min='30.0' max='80.0' color='#FFCC00' label ='Ok'/>
      <INTERVAL  min='80.0' max='100.0' color='#66FF00' label ='Good'/>
    </INTERVALS>
</DIALCHART>