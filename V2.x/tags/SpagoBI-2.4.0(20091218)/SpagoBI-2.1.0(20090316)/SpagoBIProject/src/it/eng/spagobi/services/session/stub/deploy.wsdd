<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from SessionServiceService WSDL service -->

  <service name="WSSessionService" provider="java:RPC" style="rpc" use="encoded">
      <parameter name="wsdlTargetNamespace" value="urn:spagobiwssession"/>
      <parameter name="wsdlServiceElement" value="SessionServiceService"/>
      <parameter name="schemaUnqualified" value="http://bo.session.services.spagobi.eng.it,http://xml.apache.org/xml-soap,urn:spagobiwssession,http://exceptions.session.services.spagobi.eng.it"/>
      <parameter name="wsdlServicePort" value="WSSessionService"/>
      <parameter name="className" value="it.eng.spagobi.services.session.stub.WSSessionServiceSoapBindingSkeleton"/>
      <parameter name="wsdlPortType" value="SessionService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <parameter name="allowedMethods" value="*"/>
      <parameter name="scope" value="Session"/>

      <arrayMapping
        xmlns:ns="urn:spagobiwssession"
        qname="ns:ArrayOf_soapenc_string"
        type="java:java.lang.String[]"
        innerType="cmp-ns:string" xmlns:cmp-ns="http://schemas.xmlsoap.org/soap/encoding/"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <typeMapping
        xmlns:ns="http://exceptions.session.services.spagobi.eng.it"
        qname="ns:NonExecutableDocumentException"
        type="java:it.eng.spagobi.services.session.exceptions.NonExecutableDocumentException"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <typeMapping
        xmlns:ns="http://bo.session.services.spagobi.eng.it"
        qname="ns:DocumentParameter"
        type="java:it.eng.spagobi.services.session.bo.DocumentParameter"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <typeMapping
        xmlns:ns="http://bo.session.services.spagobi.eng.it"
        qname="ns:Check"
        type="java:it.eng.spagobi.services.session.bo.Check"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <typeMapping
        xmlns:ns="http://bo.session.services.spagobi.eng.it"
        qname="ns:Document"
        type="java:it.eng.spagobi.services.session.bo.Document"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <arrayMapping
        xmlns:ns="urn:spagobiwssession"
        qname="ns:ArrayOf_tns3_DocumentParameter"
        type="java:it.eng.spagobi.services.session.bo.DocumentParameter[]"
        innerType="cmp-ns:DocumentParameter" xmlns:cmp-ns="http://bo.session.services.spagobi.eng.it"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <arrayMapping
        xmlns:ns="urn:spagobiwssession"
        qname="ns:ArrayOf_tns3_Check"
        type="java:it.eng.spagobi.services.session.bo.Check[]"
        innerType="cmp-ns:Check" xmlns:cmp-ns="http://bo.session.services.spagobi.eng.it"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <typeMapping
        xmlns:ns="http://exceptions.session.services.spagobi.eng.it"
        qname="ns:AuthenticationException"
        type="java:it.eng.spagobi.services.session.exceptions.AuthenticationException"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
      <arrayMapping
        xmlns:ns="urn:spagobiwssession"
        qname="ns:ArrayOf_tns3_Document"
        type="java:it.eng.spagobi.services.session.bo.Document[]"
        innerType="cmp-ns:Document" xmlns:cmp-ns="http://bo.session.services.spagobi.eng.it"
        encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
      />
  </service>
</deployment>
