<?xml version="1.0" encoding="UTF-8"?>
<web-app id="WebApp_ID" version="2.4" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>SpagoBITalendEngine</display-name>
	
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
		<welcome-file>index.htm</welcome-file>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>default.html</welcome-file>
		<welcome-file>default.htm</welcome-file>
		<welcome-file>default.jsp</welcome-file>
	</welcome-file-list>
	
	<filter>
		<filter-name>CAS_Filter</filter-name>
		<filter-class>
			edu.yale.its.tp.cas.client.filter.CASFilter
		</filter-class>
		<init-param>
			<param-name>
				edu.yale.its.tp.cas.client.filter.loginUrl
			</param-name>
			<param-value>
				https://localhost:8443/exo-cas-2/login
			</param-value>
		</init-param>
		<init-param>
			<param-name>
				edu.yale.its.tp.cas.client.filter.validateUrl
			</param-name>
			<param-value>
				https://localhost:8443/exo-cas-2/serviceValidate
			</param-value>
		</init-param>
    <init-param>
       <param-name>edu.yale.its.tp.cas.client.filter.serverName</param-name>
       <param-value>localhost:8443</param-value>
    </init-param>
		<init-param>
			<param-name>
				edu.yale.its.tp.cas.client.filter.proxyCallbackUrl
			</param-name>
			<param-value>
				https://localhost:8443/SpagoBIWekaEngine/proxyReceptor
			</param-value>
		</init-param>	

     <init-param>
  			 <param-name>edu.yale.its.tp.cas.client.filter.wrapRequest</param-name> 
             <param-value>true</param-value> 
       </init-param> 
       	
	</filter>
	
	
	<filter>
		<filter-name>SpagoBIAccessFilter</filter-name>
		<filter-class>it.eng.spagobi.utilities.filters.SpagoBIAccessFilter</filter-class>
	</filter>
	
	<filter-mapping>
		<filter-name>CAS_Filter</filter-name>
		<url-pattern>/JobRunService/*</url-pattern>
	</filter-mapping>
		
	<filter-mapping>
    	<filter-name>SpagoBIAccessFilter</filter-name>
    	<url-pattern>/JobRunService/*</url-pattern>
	</filter-mapping>


	<servlet>
		<servlet-name>JobRunService</servlet-name>
		<servlet-class>it.eng.spagobi.engines.talend.services.JobRunService</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>JobUploadService</servlet-name>
		<servlet-class>it.eng.spagobi.engines.talend.services.JobUploadService</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>EngineInfoService</servlet-name>
		<servlet-class>it.eng.spagobi.engines.talend.services.EngineInfoService</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>EngineInfoService</servlet-name>
		<url-pattern>/EngineInfoService</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>JobRunService</servlet-name>
		<url-pattern>/JobRunService</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>JobRunService</servlet-name>
		<url-pattern>/JobRunServiceBackEnd</url-pattern>
	</servlet-mapping>	
	
	<servlet-mapping>
		<servlet-name>JobUploadService</servlet-name>
		<url-pattern>/JobUploadService</url-pattern>
	</servlet-mapping>
	
	
	<resource-ref>
	 <res-ref-name>wm/SpagoWorkManager</res-ref-name>
 	<res-type>commonj.work.WorkManager</res-type>
 	<res-auth>Container</res-auth>
 	<res-sharing-scope>Shareable</res-sharing-scope>
	</resource-ref>
	
	
	
</web-app>
