	<MODULE name="ListModelModule" class="it.eng.spagobi.kpi.model.service.ListModelModule">
		<CONFIG pool="spagobi" rows="10" title="sbi.kpi.list.model.title">
		<QUERIES>
        <SELECT_QUERY>
          <ORDER_BY  scope="SERVICE_REQUEST" type="RELATIVE" value="FIELD_ORDER"/>
          <ORDER_BY  scope="SERVICE_REQUEST" type="RELATIVE" value="TYPE_ORDER" />
          <PARAMETER name="LIGHT_NAVIGATOR_REPLACE_LAST" scope="" type="ABSOLUTE" value="true" />
        </SELECT_QUERY>
        </QUERIES>
			<COLUMNS>
				<COLUMN label="sbi.kpi.label.name" name="NAME" />
			</COLUMNS>
			<CAPTIONS>
				<SELECT_CAPTION confirm="FALSE" image="/img/detail.gif"
					label="sbi.kpi.caption.select">
					<PARAMETER name="PAGE" scope="" type="ABSOLUTE" value="ModelPage" />
					<PARAMETER name="MODULE" scope="" type="ABSOLUTE"
						value="DetailModelModule" />
					<PARAMETER name="MESSAGE" scope="" type="ABSOLUTE" value="DETAIL_SELECT" />
					<PARAMETER name="ID" scope="LOCAL" type="RELATIVE" value="ID" />
				</SELECT_CAPTION>
				<TREE_CAPTION confirm="FALSE" image="/img/kpi/treeView16.png"
					label="sbi.kpi.caption.tree">
					<PARAMETER name="PAGE" scope="" type="ABSOLUTE" value="ModelTreePage" />
					<PARAMETER name="MODULE" scope="" type="ABSOLUTE"
						value="ListModelTreeModule" />
					<PARAMETER name="ID" scope="LOCAL" type="RELATIVE" value="ID" />
				</TREE_CAPTION>
				<DELETE_CAPTION confirm="TRUE" image="/img/erase.gif"
					label="sbi.kpi.caption.delete">
					<PARAMETER name="PAGE" scope="" type="ABSOLUTE" value="ModelPage" />
					<PARAMETER name="MODULE" scope="" type="ABSOLUTE" value="ListModelModule" />
					<PARAMETER name="MESSAGE" scope="" type="ABSOLUTE" value="LIST_DELETE" />
					<PARAMETER name="ID" scope="LOCAL" type="RELATIVE" value="ID" />
				</DELETE_CAPTION>
			</CAPTIONS>
			<BUTTONS>
				<INSERT_BUTTON confirm="TRUE" image="/img/new.png"
					label="sbi.kpi.button.insert.title">
					<PARAMETER name="PAGE" scope="" type="ABSOLUTE" value="ModelPage" />
					<PARAMETER name="MODULE" scope="" type="ABSOLUTE"
						value="DetailModelModule" />
					<PARAMETER name="MESSAGE" scope="" type="ABSOLUTE" value="DETAIL_NEW" />
				</INSERT_BUTTON>
				<BACK_BUTTON confirm="FALSE" image="/img/back.png"
					label="sbi.kpi.button.back.title" onlyPortletRunning="true">
					<PARAMETER name="ACTION_NAME" scope="" type="ABSOLUTE"
						value="LOGIN_ACTION_KPI_CONFIGURATION" />
					<PARAMETER name="LIGHT_NAVIGATOR_RESET" scope="" type="ABSOLUTE"
						value="true" />
				</BACK_BUTTON>
			</BUTTONS>
		</CONFIG>
	</MODULE>
	
