				    <VALIDATOR validatorName="REGEXP" arg0="^([a-zA-Z0-9\s\-\_\(\)])*$"/>
					<VALIDATOR validatorName="MAXLENGTH" arg0="100"/>
				</FIELD>
			</FIELDS>
		</VALIDATION>
	</SERVICE>

