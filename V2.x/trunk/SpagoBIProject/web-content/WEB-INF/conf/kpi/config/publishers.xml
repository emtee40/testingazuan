<?xml version="1.0" encoding="ISO-8859-1"?>
<PUBLISHERS>
  <PUBLISHER name="KpiPublisher">
    <RENDERING channel="PORTLET" type="JAVA" mode="">
      <RESOURCES>
        <ITEM prog="0" resource="it.eng.spago.presentation.DefaultPublisherDispatcher">
          <CONFIG>
            <CHECKS>
              <CHECK target="ListKpiPublisher">
                <CONDITIONS>
                  <PARAMETER name="module" scope="SERVICE_REQUEST" value="AF_NOT_DEFINED" />
                </CONDITIONS>
              </CHECK>
              <CHECK target="ListKpiPublisher">
                <CONDITIONS>
                  <PARAMETER name="module" scope="SERVICE_REQUEST" value="ListKpiModule" />
                </CONDITIONS>
              </CHECK>
              <CHECK target="ListKpiPublisher">
                <CONDITIONS>
                  <PARAMETER name="message" scope="SERVICE_REQUEST" value="DETAIL_UPDATE" />
                </CONDITIONS>
              </CHECK>
              <CHECK target="DetailKpiPublisher">
                <CONDITIONS>
                  <PARAMETER name="module" scope="SERVICE_REQUEST" value="DetailKpiModule" />
                </CONDITIONS>
              </CHECK>
            </CHECKS>
          </CONFIG>
        </ITEM>
      </RESOURCES>
    </RENDERING>
  </PUBLISHER>
  <PUBLISHER name="ListKpiPublisher">
    <RENDERING channel="PORTLET" mode="FORWARD" type="JSP">
      <RESOURCES>
        <ITEM mode="VIEW" resource="/jsp/kpi/config/listKpi.jsp" />
      </RESOURCES>
    </RENDERING>
  </PUBLISHER>
  <PUBLISHER name="DetailKpiPublisher">
    <RENDERING channel="PORTLET" mode="FORWARD" type="JSP">
      <RESOURCES>
        <ITEM mode="VIEW" resource="/jsp/kpi/config/detailKpi.jsp" />
      </RESOURCES>
    </RENDERING>
  </PUBLISHER>
</PUBLISHERS>