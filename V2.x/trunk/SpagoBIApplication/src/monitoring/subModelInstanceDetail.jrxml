	<queryString><![CDATA[SELECT COUNT(TV.THRESHOLD_ID) AS THREV, MODELINSTANCE.KPI_MODEL_INST AS KPIMODELINST,
MODELINSTANCE.NAME AS MODELINSTANCENAME,
KPIINSTANCE.WEIGHT AS KPIINSTANCEWEIGHT,
KPI.NAME AS KPINAME,
KPI.CODE AS KPICODE,
KPI.DOCUMENT_LABEL AS KPIDOCUMENTLABEL,
THRESHOLD.THRESHOLD_ID AS THRESHOLDID,
THRESHOLD.NAME AS THRESHOLDNAME,
THRESHOLD.CODE AS THRESHOLDCODE,
SBIDATASET.NAME AS DATASET,
MODEL.KPI_MODEL_CD AS CODE
FROM SBI_KPI_MODEL MODEL, SBI_KPI_MODEL_INST MODELINSTANCE
LEFT JOIN SBI_KPI_INSTANCE KPIINSTANCE
ON MODELINSTANCE.ID_KPI_INSTANCE = KPIINSTANCE.ID_KPI_INSTANCE
LEFT JOIN SBI_THRESHOLD THRESHOLD
ON KPIINSTANCE.THRESHOLD_ID = THRESHOLD.THRESHOLD_ID
LEFT JOIN SBI_DOMAINS CHARTYPE
ON KPIINSTANCE.CHART_TYPE_ID = CHARTYPE.VALUE_ID
LEFT JOIN SBI_THRESHOLD_VALUE TV
ON TV.THRESHOLD_ID = THRESHOLD.THRESHOLD_ID
LEFT JOIN SBI_KPI KPI
ON KPI.KPI_ID = KPIINSTANCE.KPI_ID
LEFT JOIN SBI_DATA_SET SBIDATASET
ON KPI.DS_ID = SBIDATASET.DS_ID
WHERE MODELINSTANCE.KPI_MODEL_INST_PARENT = $P{KPI_MODEL_INST}
AND MODELINSTANCE.KPI_MODEL_ID = MODEL.KPI_MODEL_ID
GROUP BY MODELINSTANCE.KPI_MODEL_INST, MODELINSTANCE.NAME, KPIINSTANCE.WEIGHT, KPI.NAME, KPI.CODE, KPI.DOCUMENT_LABEL, 
THRESHOLD.THRESHOLD_ID, THRESHOLD.NAME, THRESHOLD.CODE, SBIDATASET.NAME,
MODEL.KPI_MODEL_CD
ORDER BY MODEL.KPI_MODEL_CD]]></queryString>

