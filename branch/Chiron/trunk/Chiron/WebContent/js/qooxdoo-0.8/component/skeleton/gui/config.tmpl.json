{
  "name"    : "${Name}",

  "include" :
  [
    {
      "path" : "${REL_QOOXDOO_PATH}/tool/data/config/application.json"
    }
  ],

  "let" :
  {
    "APPLICATION"  : "${Namespace}",
    "QOOXDOO_PATH" : "${REL_QOOXDOO_PATH}/framework",
    "QOOXDOO_URI"  : "../$${QOOXDOO_PATH}",
    "QXTHEME"      : "qx.theme.Modern",
    "API_EXCLUDE"  : ["qx.legacy.*"],
    "LOCALES"      : [ "en" ],
    "ROOT"         : "."
  }
}
