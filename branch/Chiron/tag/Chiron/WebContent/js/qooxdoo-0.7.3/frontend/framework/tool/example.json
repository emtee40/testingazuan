{
  "common" :
  {
    "library" :
    [
      {
        "encoding" : "utf-8",
        "path" : "framework/source",
        "uri" : "../../../framework/source"
      },

      {
        "encoding" : "utf-8",
        "path" : "application/apiviewer/source",
        "uri" : "../source"
      },

      {
        "encoding" : "utf-8",
        "path" : "application/feedreader/source",
        "uri" : "../source"
      },

      {
        "encoding" : "utf-8",
        "path" : "application/webmail/source",
        "uri" : "../source"
      },

      {
        "encoding" : "utf-8",
        "path" : "application/showcase/source",
        "uri" : "../source"
      }
    ],

    "require" :
    {
      "qx.log.Logger" : ["qx.log.appender.Native"]
    },

    "cache" :
    {
      "path" : "cache2"
    }
  },




  "api-feedreader" :
  {
    "extend" : ["common"],
    "include" : ["feedreader.Application"],

    "api" :
    {
      "path" : "application/apiviewer/source/script"
    }
  },

  "api-webmail" :
  {
    "extend" : ["common"],
    "include" : ["webmail.Application"],

    "api" :
    {
      "path" : "application/apiviewer/source/script"
    }
  },

  "api-create" :
  {
    "extend" :["common"],
    "include" : ["apiviewer.Application", "qx.theme.ClassicRoyale"],

    "settings" :
    {
      "qx.minLogLevel" : "700",
      "qx.theme" : "qx.theme.ClassicRoyale",
      "qx.application" : "apiviewer.Application",
      "qx.resourceUri" : "../../../framework/source/resource",
      "apiviewer.title" : "Generator2 API Viewer"
    },

    "variants" :
    {
      "qx.debug" : ["off"]
    },

    "source" :
    {
      "file" : "application/apiviewer/source/script/apiviewer.js"
    }
  },




  "source-all" :
  {
    "extend" : ["common"],

    "source" :
    {
      "file" : "source-all.js"
    }
  },

  "compile-all" :
  {
    "extend" : ["common"],

    "compile" :
    {
      "file" : "build-all.js"
    }
  },

  "localize-all" :
  {
    "extend" : ["common"],

    "localize" :
    {
      "locales" : ["de","en","fr"]
    }
  },




  "localize-update-qx" :
  {
    "extend" : ["common"],
    "include" : ["=qx.core.Object"],
    "localize" :
    {
      "update" : ["qx"]
    }
  },

  "localize-update-feedreader" :
  {
    "extend" : ["common"],
    "include" : ["=qx.core.Object"],
    "localize" :
    {
      "update" : ["feedreader"]
    }
  },





  "source-feedreader" :
  {
    "extend" : ["common"],
    "include" : ["feedreader.*", "qx.theme.Ext"],

    "settings" :
    {
      "qx.application" : "feedreader.Application",
      "qx.resourceUri" : "../../../framework/source/resource"
    },

    "source" :
    {
      "file" : "feedreader-source.js",
      "gzip" : "true"
    },

    "localize" :
    {
      "locales" : ["en", "de", "es", "it"]
    }
  },

  "source-feedreader-parts" :
  {
    "extend" : ["source-feedreader"],

    "packages" :
    {
      "size" : 5,

      "parts" :
      {
        "core" : ["feedreader.Application"],
        "table" : ["feedreader.view.Table"],
        "tree" : ["feedreader.view.Tree"]
      }
    },

    "localize" :
    {
      "locales" : ["en", "de", "es", "it"]
    }
  },

  "compile-feedreader" :
  {
    "extend" : ["common"],
    "include" : ["feedreader.*", "qx.theme.Ext"],

    "settings" :
    {
      "qx.application" : "feedreader.Application",
      "qx.resourceUri" : "../../../framework/source/resource"
    },

    "compile" :
    {
      "file" : "feedreader-build.js",
      "gzip" : "true",
      "optimize" : [ "variables", "basecalls" ]
    }
  },

  "compile-feedreader-parts" :
  {
    "extend" : ["compile-feedreader"],

    "packages" :
    {
      "size" : 5,

      "parts" :
      {
        "core" : ["feedreader.Application"],
        "table" : ["feedreader.view.Table"],
        "tree" : ["feedreader.view.Tree"]
      }
    }
  },






  "source-apiviewer" :
  {
    "extend" : ["common"],
    "include" : ["apiviewer.*", "qx.theme.ClassicRoyale"],

    "settings" :
    {
      "qx.application" : "apiviewer.Application",
      "qx.resourceUri" : "../../../framework/source/resource"
    },

    "source" :
    {
      "file" : "apiviewer-source.js",
      "gzip" : "true"
    }
  },

  "compile-apiviewer" :
  {
    "extend" : ["common"],
    "include" : ["apiviewer.*", "qx.theme.ClassicRoyale"],

    "settings" :
    {
      "qx.application" : "apiviewer.Application",
      "qx.resourceUri" : "../../../framework/source/resource"
    },

    "compile" :
    {
      "file" : "apiviewer-build.js",
      "gzip" : "true",
      "optimize" : [ "variables", "basecalls" ]
    }
  },

  "compile-apiviewer-variants" :
  {
    "extend" : ["common"],
    "include" : ["apiviewer.*", "qx.theme.ClassicRoyale"],

    "compile" :
    {
      "file" : "apiviewer-variants-build-$variant.js",
      "optimize" : [ "variables", "basecalls" ]
    },

    "variants" :
    {
      "qx.debug" : ["off"],
      "qx.aspects" : ["off"],
      "qx.client" : ["gecko", "mshtml"]
    }
  },

  "localize-apiviewer-parts" :
  {
    "extend" : ["common"],

    "localize" :
    {
      "locales" : ["de","en","fr"]
    },

    "variants" :
    {
      "qx.debug" : ["off"]
    },

    "packages" :
    {
      "collapse" : ["core"],
      "size" : 10,

      "parts" :
      {
        "core" : ["apiviewer.Application","qx.theme.ClassicRoyale"],
        "viewer" : ["apiviewer.Viewer"],
        "content" : ["apiviewer.ui.ClassViewer","apiviewer.ui.PackageViewer"]
      }
    }
  },

  "source-apiviewer-parts" :
  {
    "extend" : ["common"],

    "source" :
    {
      "file" : "source-apiviewer-pkgs.js"
    },

    "variants" :
    {
      "qx.debug" : ["off"]
    },

    "packages" :
    {
      "collapse" : ["core"],
      "size" : 10,

      "parts" :
      {
        "core" : ["apiviewer.Application","qx.theme.ClassicRoyale"],
        "viewer" : ["apiviewer.Viewer"],
        "content" : ["apiviewer.ui.ClassViewer","apiviewer.ui.PackageViewer"]
      }
    }
  },

  "compile-apiviewer-parts" :
  {
    "extend" : ["common"],

    "compile" :
    {
      "file" : "build-apiviewer-pkgs-{qx.client}.js",
      "optimize" : ["basecalls", "variables"]
    },

    "variants" :
    {
      "qx.debug" : ["off"],
      "qx.client" : ["gecko", "mshtml"]
    },

    "packages" :
    {
      "collapse" : ["core"],
      "size" : 10,

      "parts" :
      {
        "core" : ["apiviewer.Application","qx.theme.ClassicRoyale"],
        "viewer" : ["apiviewer.Viewer"],
        "content" : ["apiviewer.ui.ClassViewer","apiviewer.ui.PackageViewer"]
      }
    }
  },

  "compile-apiviewer-parts-noqx" :
  {
    "extend" : ["common"],

    "compile" :
    {
      "file" : "build-apiviewer-pkgs-noqx-{qx.client}.js"
    },

    "variants" :
    {
      "qx.debug" : ["off"],
      "qx.client" : ["gecko", "mshtml"]
    },

    "include" : ["apiviewer.Application"],
    "exclude" : ["=qx.*"],

    "packages" :
    {
      "collapse" : ["core"],

      "parts" :
      {
        "core" : ["apiviewer.Application","qx.theme.ClassicRoyale"],
        "viewer" : ["apiviewer.Viewer"],
        "content" : ["apiviewer.ui.ClassViewer","apiviewer.ui.PackageViewer"],
        "feed" : ["feedreader.Application"]
      }
    }
  }
}
